   <taskbody>
      <steps class="- topic/ol task/steps ">
         <step class="- topic/li task/step ">
            <cmd class="- topic/ph task/cmd ">Go to <menucascade class="+ topic/ph ui-d/menucascade ">
                  <uicontrol class="+ topic/ph ui-d/uicontrol ">Setup</uicontrol>
                  <uicontrol class="+ topic/ph ui-d/uicontrol ">App Setup</uicontrol>
                  <uicontrol class="+ topic/ph ui-d/uicontrol ">Objects</uicontrol>
                  <uicontrol class="+ topic/ph ui-d/uicontrol ">Deal Guidance Dimension</uicontrol>
                  <uicontrol class="+ topic/ph ui-d/uicontrol ">Business Objects</uicontrol>
               </menucascade>.</cmd>
         </step>
         <step class="- topic/li task/step ">
            <cmd class="- topic/ph task/cmd ">Click <uicontrol class="+ topic/ph ui-d/uicontrol ">New</uicontrol> next to the <uicontrol class="+ topic/ph ui-d/uicontrol ">Values</uicontrol>
               section.</cmd>
         </step>
         <step class="- topic/li task/step ">
            <cmd class="- topic/ph task/cmd ">Add the following two object references:</cmd>
            <info class="- topic/itemgroup task/info ">
               <codeph class="+ topic/ph pr-d/codeph ">Apttus_Config2__LineItem__c
                  Apttus_Config2__ProductConfiguration__c</codeph>
               <fig id="fig_qcf_3f3_rcc" class="- topic/fig ">
                  <image height="250"
                         placement="break"
