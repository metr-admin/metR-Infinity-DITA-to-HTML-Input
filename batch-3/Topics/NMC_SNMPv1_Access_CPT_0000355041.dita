                                 <codeph translate="no" class="+ topic/ph pr-d/codeph ">?</codeph>
                              </entry>
                              <entry class="- topic/entry "/>
                              <entry class="- topic/entry ">
                                 <ph class="- topic/ph ">Shows the command help.</ph>
                              </entry>
                           </row>
                           <row class="- topic/row ">
                              <entry class="- topic/entry ">
                                 <ph class="- topic/ph ">SNMPv1
access</ph>
                              </entry>
                              <entry class="- topic/entry ">
                                 <codeph translate="no" class="+ topic/ph pr-d/codeph ">–S</codeph>
                              </entry>
                              <entry class="- topic/entry ">
                                 <codeph translate="no" class="+ topic/ph pr-d/codeph ">enable | disable</codeph>
                              </entry>
