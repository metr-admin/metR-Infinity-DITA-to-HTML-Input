   <conbody>
      <hazardstatement conref="../Library/Warnings_Library_0000325173.dita#war_lib/parts_seperate_package"
                       class="+ topic/note hazard-d/hazardstatement ">
         <messagepanel class="+ topic/ul hazard-d/messagepanel ">
