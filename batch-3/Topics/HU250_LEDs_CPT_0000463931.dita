                        <sli class="- topic/sli ">
                           <b class="+ topic/ph hi-d/b ">Red: </b>Alarm active</sli>
                        <sli class="- topic/sli ">
                           <b class="+ topic/ph hi-d/b ">Off: </b>OK</sli>
                     </sl>
                     <sl platform="Modular-DC Modular-Inverter Modular-AC Prime-UPS"
