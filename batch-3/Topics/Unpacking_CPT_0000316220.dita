      <hazardstatement conref="../Library/Warnings_Library_0000325173.dita#war_lib/moisture_inside_system"
                       class="+ topic/note hazard-d/hazardstatement ">
         <messagepanel class="+ topic/ul hazard-d/messagepanel ">
