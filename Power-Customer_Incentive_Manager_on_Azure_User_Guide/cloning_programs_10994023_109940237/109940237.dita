   <title class="- topic/title ">Cloning Programs</title>
   <prolog class="- topic/prolog ">
      <author class="- topic/author ">Metapercept</author>
      <critdates class="- topic/critdates ">
         <created date="2025-11-10+05:30" class="- topic/created "/>
