         <othermeta name="title" content="Withdrawing a Campaign"/>
         <othermeta name="shortdesc" content="Covers the workflow for withdrawing a campaign from the public portal, including temporary and permanent withdrawal options and stakeholder notifications."/>
         <othermeta name="language" content="en-GB"/>
         <othermeta name="version" content="1.0.0"/>
